{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "profile.jpg",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/profile.jpg",
      "dest": "/profile.jpg"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
